<!-- Main Section -->
<main id="main-site">
        <div class="container">
            <div class="box-nav d-flex justify-between">
                <a href="/add-user" class="border-shadow" >
                    <span>New User<i class="fa-solid fa-user"></i></span>
                </a>
            </div>
            <!-- Form-Handling -->
            <form action="/" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Username</th>
                        <th>Email</th>
                        <th>Gender</th>
